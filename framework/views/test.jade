extends layout

block pageTitle
    h1 $$\int_{-1}^3 3x^2\,\mathrm{d}x = 28$$
    h1 $$2 \Delta x\cdot\Delta p_x \geqslant \hbar$$
block pageContent
//
    ul
    each test in tests.list
        //create a new well for each test model
        .well
            h2 #{test.name}
            img(src='#{test.photo}')
            h3 Yolo questions
            h5
                ul
                each problem in test.problems
                    li=problem
